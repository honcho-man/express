                a(href="/gallery") Gallery 
              .hide.m-1
                i.lni.lni-bolt
            li(style="width:90px;").d-flex
              .myDIV.m-1
                a(href="/products") Products 
              .hide.m-1
                i.lni.lni-bolt
            li(style="width:80px;").d-flex
              .myDIV.m-1
                a(href="/contact") Contact 
              .hide.m-1
                i.lni.lni-bolt
